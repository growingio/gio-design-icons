{
  "name": "@gio-design/icons",
  "version": "20.12.0",
  "description": "GrowingIO Design semantic vector icons",
  "author": "GrowingIO Frontend Team <eng-frontend@growingio.com>",
  "homepage": "https://growingio.design/resources/icons",
  "license": "Apache-2.0",
  "main": "es/index.js",
  "files": [
    "dist",
    "es"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growingio/gio-design-icons.git"
  },
  "scripts": {
    "build": "gio-rewire lib compile",
    "build-sotrybook": "build-storybook -s public",
    "generate": "npx @svgr/cli --template templates/svgTemplate.ts --out-dir src svgs",
    "gen-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "gen-version": "conventional-changelog -p angular > VERSION.md",
    "preversion": "git fetch --tags",
    "postversion": "yarn gen-version && yarn gen-changelog",
    "storybook": "yarn build && start-storybook -p 6006 -s public",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "bugs": {
    "url": "https://github.com/growingio/gio-design-icons/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/addon-storysource": "^6.1.10",
    "@storybook/node-logger": "^6.1.10",
    "@storybook/react": "^6.1.10",
    "@svgr/cli": "^5.4.0",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "commitizen": "^4.2.2",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "gio-rewire": "^1.0.4",
    "husky": "^4.3.6",
    "less": "^4.0.0",
    "less-loader": "^7.2.0",
    "react": "^17.0.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^17.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
